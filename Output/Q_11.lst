SQL> SELECT PLAYER_NAME, TEAM_INITIALS,HOME_TEAM_NAME, SUM(SUM_GOAL) AS Total_Goal from (SELECT PLAYER_NAME,TEAM_INITIALS, LENGTH(EVENT) - LENGTH(REPLACE(EVENT, 'G', '')) AS SUM_GOAL,HOME_TEAM_NAME FROM (WORLD_CUP_PLAYERS LEFT OUTER JOIN (SELECT DISTINCT HOME_TEAM_NAME, HOME_TEAM_INITIALS FROM WORLD_CUP_MATCHES) ON TEAM_INITIALS = HOME_TEAM_INITIALS) WHERE LENGTH(EVENT) - LENGTH(REPLACE(EVENT, 'G', ''))>0) GROUP BY PLAYER_NAME, TEAM_INITIALS, HOME_TEAM_NAME HAVING SUM(SUM_GOAL)>2 ORDER BY Total_Goal DESC;

