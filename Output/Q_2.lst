SQL> WITH temp1 AS (
  2      SELECT
  3          WM.YEAR,
  4          WP.PLAYER_NAME,
  5          WP.TEAM_INITIALS,
  6          WP.SHIRT_NUMBER,
  7          WP.POSITION,
  8          SUM(LENGTH(WP.EVENT) - LENGTH(REPLACE(WP.EVENT, 'G', ''))) AS TOTAL_GOAL,
  9          WP.MATCHID,
 10          WP.ROUNDID
 11      FROM
 12          WORLD_CUP_PLAYERS WP
 13      INNER JOIN (
 14          SELECT DISTINCT
 15              YEAR,
 16              MATCHID, ROUNDID,
 17              HOME_TEAM_INITIALS AS COUNTRY
 18          FROM
 19              WORLD_CUP_MATCHES
 20          WHERE
 21              HOME_TEAM_GOALS > AWAY_TEAM_GOALS
 22          UNION
 23          SELECT DISTINCT
 24              YEAR,
 25              MATCHID, ROUNDID,
 26              AWAY_TEAM_INITIALS AS COUNTRY
 27          FROM
 28              WORLD_CUP_MATCHES
 29          WHERE
 30              HOME_TEAM_GOALS < AWAY_TEAM_GOALS
 31      ) WM ON WP.MATCHID = WM.MATCHID
 32             AND WP.ROUNDID = WM.ROUNDID
 33             AND WP.TEAM_INITIALS = WM.COUNTRY
 34      WHERE
 35          LENGTH(WP.EVENT) - LENGTH(REPLACE(WP.EVENT, 'G', '')) > 0
 36      GROUP BY
 37          WM.YEAR,
 38          WP.PLAYER_NAME,
 39          WP.MATCHID,
 40          WP.ROUNDID,
 41          WP.TEAM_INITIALS,
 42          WP.SHIRT_NUMBER,
 43          WP.POSITION
 44  ),
 45  temp2 AS (
 46      SELECT
 47          MATCHID,
 48          ROUNDID,
 49          MAX(TOTAL_GOAL) as MAX_TOTAL_GOAL
 50      FROM
 51          temp1
 52      GROUP BY
 53          MATCHID,
 54          ROUNDID
 55  )
 56  SELECT
 57      PLAYER_NAME,
 58      TEAM_INITIALS AS COUNTRY,
 59      SHIRT_NUMBER,
 60      POSITION,
 61      TOTAL_GOAL,
 62      temp1.MATCHID,
 63      temp1.ROUNDID,
 64      YEAR
 65  FROM
 66      temp1
 67  INNER JOIN
 68      temp2 ON temp1.MATCHID = temp2.MATCHID
 69              AND temp1.ROUNDID = temp2.ROUNDID
 70              AND temp1.TOTAL_GOAL = temp2.MAX_TOTAL_GOAL
 71  ORDER BY
 72      temp1.TOTAL_GOAL DESC;

